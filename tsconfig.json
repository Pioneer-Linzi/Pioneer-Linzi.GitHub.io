{
    "compilerOptions": {
        "outDir": "./dist/",
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es5",
        "sourceMap": true,
        "jsx": "react",
        "lib": ["es2015", "dom", "es2015.promise"]  //编译过程中需要引入的库文件的列表
    },
    "include": [
        "./src/**/*"
    ],
    "exclude": ["dist", "node_modules"]  //编译时排除 dist、node_modules文件夹
}
